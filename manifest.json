{
    "name": "Whatstransltor",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "pop.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
        {
          "matches": ["https://web.whatsapp.com/*"], 
          "js": ["content.js"]
        }
    ]
}
